syms x z lambda
y = x+10;

fxz = sqrt(50*(50-x)*(50-y)*(50-z));
gxz = x+y+z-100;
simplify(fxz);

f_x = diff(fxz,x);
f_z = diff(fxz,z);

pretty(simplify(f_x))
pretty(simplify(f_z)) 

eq1 = f_x + lambda*diff(gxz,x) == 0;
eq2 = f_z + lambda*diff(gxz,z) == 0;
eq3 = gxz == 0;

Sol = solve([eq1,eq2,eq3],[x,z,lambda]);

x_sol = double(Sol.x)
z_sol = double(Sol.z)
lambda_sol = double(Sol.lambda)

fxz_sol = sqrt(50*(50-x_sol(2))*(50-y)*(50-z));
